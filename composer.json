{
    "name": "joomla/keychain",
    "type": "joomla-package",
    "description": "Joomla Keychain Package",
    "keywords": ["joomla", "framework", "keychain"],
    "homepage": "https://github.com/joomla-framework/keychain",
    "license": "LGPL-2.1-or-later",
    "require": {
        "php": "~8.1.0",
        "joomla/crypt": "3.0.x-dev",
        "joomla/registry": "3.0.x-dev"
    },
    "require-dev": {
        "joomla/console": "3.0.x-dev",
        "phpunit/phpunit": "^9.5.28",
        "squizlabs/php_codesniffer": "~3.7.1"
    },
    "suggest": {
        "joomla/console": "For managing a keychain through a command line interface"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Keychain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Keychain\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev",
            "dev-3.x-dev": "3.0-dev"
        }
    }
}
