{
    "name": "joomla/keychain",
    "type": "joomla-package",
    "description": "Joomla Keychain Package",
    "keywords": ["joomla", "framework", "keychain"],
    "homepage": "https://github.com/joomla-framework/keychain",
    "license": "LGPL-2.1+",
    "require": {
        "php": ">=5.3.10",
        "joomla/registry": "~2.0@dev"
    },
    "require-dev": {
        "joomla/application": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "joomla/application": "Install joomla/application if you would like to use the Keychain Management Utility."
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Keychain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Keychain\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    },
    "bin": ["bin/keychain"]
}
